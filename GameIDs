local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/ThacG/Orion/main/mobile')))()
local Window = OrionLib:MakeWindow({Name = "a dusty trip | Main Menu", HidePremium = false, SaveConfig = true, ConfigFolder = "dustytrip"})

local MainTab = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

MainTab:AddButton({
	Name = "The Lobby",
	Callback = function()
      	https://raw.githubusercontent.com/Zaid12334/a-dusty-trip/main/Lobby
  	end    
})

MainTab:AddButton({
	Name = "The Desert",
	Callback = function()
      	https://raw.githubusercontent.com/Zaid12334/a-dusty-trip/main/Desert
  	end    
})

MainTab:AddButton({
	Name = "The Plains",
	Callback = function()
      	https://raw.githubusercontent.com/Zaid12334/a-dusty-trip/main/Desert
  	end    
})

OrionLib:Init()
